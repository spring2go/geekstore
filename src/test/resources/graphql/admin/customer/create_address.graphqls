mutation CreateAddress($id: ID!, $input: CreateAddressInput!) {
    adminCreateCustomerAddress(customerId: $id, input: $input) {
        id
        fullName
        company
        streetLine1
        streetLine2
        city
        province
        postalCode
        phoneNumber
        defaultShippingAddress
        defaultBillingAddress
    }
}